IniteGame game = new IniteGame();

		List<Player> players = new ArrayList<>();

		Player player0 = playerRepositry.findAll().get(0);
		Player player1 = playerRepositry.findAll().get(1);

		// random chooce chars x or o.
		player0.setPlayerState(new PlayerState());
		player1.setPlayerState(new PlayerState());

		chooceRandomChar(player0);

		if (player0.getPlayerState().getChars() == 'x')
			player1.getPlayerState().setChars('o');

		if (player0.getPlayerState().getChars() == 'o')
			player1.getPlayerState().setChars('x');

		player0.getPlayerState().setState(PlayerState.PLAYER_STATE.READY.name());
		player1.getPlayerState().setState(PlayerState.PLAYER_STATE.READY.name());

		players.add(player0);
		players.add(player1);

		GameSetting gameSetting = new GameSetting(3, GAME_STATE.START.name(), players);
		game.setGameSetting(gameSetting);

		PlayerStrategy strategy = new PlayerStrategy();
		strategy.defaultArray();

		char[][] chars1 = { { 'o', 'o', 'x' }, { 'x', 'd', 'o' }, { 'x', 'o', 'd' } };

		try {
			strategy.setPlayerState(player0.getPlayerState());
			strategy.replaceChars(chars1);
			strategy.doCompare();
			

			System.out.println(
					strategy.getPlayerState().getState() + " +++ ++ + ++ " + strategy.getPlayerState().getChars());
			Thread.sleep(500);

			strategy.setPlayerState(player1.getPlayerState());
			strategy.replaceChars(chars1);
			strategy.doCompare();
			
			if(player0.getPlayerState()
					.getState() == PLAYER_STATE.WINNER.name())
				player1.getPlayerState().setState(PLAYER_STATE.LOSER.name());
			
			strategy.setPlayerState(player1.getPlayerState());
			System.out.println(
					strategy.getPlayerState().getState() + " +++ ++ + ++ " + strategy.getPlayerState().getChars());
		} catch (Exception e) {
			// TODO: handle exception
		} finally {
			game.setPlayerStrategy(strategy);
			try {
				this.save(game);
			} catch (Exception e2) {
				System.out.println(e2.getMessage());
			}
		}
